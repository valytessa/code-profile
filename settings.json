	"vim.whichwrap": "b,s",
	"vim.insertModeKeyBindings": [
		{
			"before": ["j", "j"],
			"after": ["<Esc>"]
		},
		{
			"before": ["j", "k"],
			"after": ["<Esc>"]
		},
		{
			"before": ["k", "k"],
			"after": ["<Esc>", "l"]
		},
		{
			"before": [";", ";"],
			"after": ["<Esc>", "A", ";", "<Esc>"]
		},
		{
			"before": [",", ","],
			"after": ["<Esc>", "A", ",", "<Esc>"]
		}
	],
	"vim.normalModeKeyBindings": [
		{
			"before": ["[", "d"],
			"commands": ["editor.action.marker.prev"]
		},
		{
			"before": ["]", "d"],
			"commands": ["editor.action.marker.next"]
		},
		{
			"before": ["<C-i>"],
			"commands": ["scrollLineUp"]
		},
		{
			"before": ["<leader>", "1"],
			"commands": ["workbench.files.action.createFileFromExplorer"]
		},
		{
			"before": ["<leader>", "2"],
			"commands": ["workbench.files.action.createFolderFromExplorer"]
		},
		{
			"before": ["<leader>", ";"],
			"commands": ["endline.new"],
			"silent": true
		},
		{
			"before": ["<leader>", "q"],
			"commands": ["workbench.action.closeActiveEditor"]
		},
		{
			"before": ["<leader>", "f"],
			"commands": ["workbench.action.toggleSidebarVisibility"]
		},
		{
			"before": ["<leader>", "o"],
			"commands": ["workbench.action.quickOpen"]
		},
		{
			"before": ["<leader>", "p"],
			"commands": ["workbench.action.showCommands"]
		},
		{
			"before": ["<leader>", "r"],
			"commands": ["editor.action.rename"]
		},
		{
			"before": ["<C-m>"],
			"after": ["<leader>", "<leader>", "w"]
		},
		{
			"before": ["K"],
			"commands": ["lineBreakInsert"],
			"silent": true
		},
		{
			"before": ["<leader>", "j"],
			"commands": ["editor.action.insertLineAfter", "cursorUp"]
		},
		{
			"before": ["<leader>", "k"],
			"commands": ["editor.action.insertLineBefore", "cursorDown"]
		},
		{
			"before": ["<leader>", "d"],
			"after": ["d", "d"]
		}
	],
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["<leader>", "n"],
			"commands": ["newFile.createNewFile"]
		},
		{
			"before": ["<leader>", "h"],
			"commands": ["editor.action.showHover"]
		},
		{
			"before": ["H"],
			"after": ["^"]
		},
		{
			"before": ["s"],
			"after": ["c", "l"]
		},
		{
			"before": ["m"],
			"after": ["s"]
		},
		{
			"before": ["L"],
			"after": ["$"]
		},
		{
			"before": ["z", "o"],
			"after": ["o", "<Esc>", "o"]
		},
		{
			"before": ["<C-p>"],
			"commands": ["workbench.action.quickOpen"]
		},
		{
			"before": ["<leader>", "s"],
			"commands": ["workbench.action.reloadWindow"]
		},
		{
			"before": ["<leader>", "<Esc>"],
			"commands": [":nohl"]
		},
		{
			"before": ["<C-]>"],
			"commands": ["editor.action.revealDefinition"]
		},
		{
			"before": ["<C-h>"],
			"commands": ["workbench.action.navigateLeft"]
		},
		{
			"before": ["<C-l>"],
			"commands": ["workbench.action.navigateRight"]
		},
		{
			"before": ["<C-j>"],
			"commands": ["workbench.action.navigateDown"]
		},
		{
			"before": ["<C-k>"],
			"commands": ["workbench.action.navigateUp"]
		}
	],
	"vim.visualModeKeyBindings": [
		{
			"before": [">"],
			"commands": ["editor.action.indentLines"]
		},
		{
			"before": ["<"],
			"commands": ["editor.action.outdentLines"]
		},
		{
			"before": [";"],
			"after": ["<Esc>"]
		}
	],
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": ["p"],
			"after": ["p", "g", "v", "y"]
		},
		{
			"before": ["s"],
			"after": ["c"]
		},
		{
			"before": ["m"],
			"after": ["s"]
		}
	],
